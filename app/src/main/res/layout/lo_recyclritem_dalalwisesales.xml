<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/_5jdp"
    android:layout_marginLeft="@dimen/_8jdp"
    android:layout_marginRight="@dimen/_8jdp"
    android:layout_marginTop="@dimen/_5jdp"
    card_view:cardCornerRadius="3dp"
    card_view:cardElevation="2dp"
    tools:ignore="Missingprefix">

    <RelativeLayout
        android:id="@+id/rltvlyot_recylrvw_dalalwssls_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/_10jdp"
        android:paddingTop="@dimen/_10jdp">

        <TextView
            android:id="@+id/tv_recylrvw_dalalwssls_firstchr"
            android:layout_width="@dimen/_40jdp"
            android:layout_height="@dimen/_40jdp"
            android:layout_marginLeft="@dimen/_8jdp"
            android:layout_marginRight="@dimen/_8jdp"
            android:background="@drawable/round_drwbl"
            android:gravity="center"
            android:padding="@dimen/_5jdp"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:textSize="@dimen/_18jdp"
            tools:text="a" />

            <!--Content-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@id/tv_recylrvw_dalalwssls_firstchr"
                android:layout_toRightOf="@id/tv_recylrvw_dalalwssls_firstchr"
                android:layout_marginEnd="@dimen/_8jdp"
                android:layout_marginRight="@dimen/_8jdp"
                android:gravity="center_vertical"
                android:orientation="vertical">

                <!--Dalal Name-->
                <TextView
                    android:id="@+id/tv_recylrvw_dalalwssls_dalalname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAllCaps="true"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    tools:text="ALPESH - MR (A00866)" />

                <!--Madhur & Other-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5jdp"
                    android:baselineAligned="false"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <!--Madhur-->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/madhur_sml"
                            android:textColor="@color/gray72"
                            android:textSize="@dimen/_12jdp" />

                        <TextView
                            android:id="@+id/tv_recylrvw_dalalwssls_madhur"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_3jdp"
                            android:layout_marginLeft="@dimen/_5jdp"
                            android:layout_marginRight="@dimen/_3jdp"
                            android:layout_marginStart="@dimen/_5jdp"
                            android:gravity="end"
                            android:textAllCaps="true"
                            android:textColor="@color/gray800"
                            android:textSize="@dimen/_12jdp"
                            android:textStyle="bold"
                            tools:text="10.00" />
                    </LinearLayout>
                    <!--END Madhur-->

                    <!--Other-->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_2jdp"
                        android:layout_marginStart="@dimen/_2jdp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/other_sml"
                            android:textColor="@color/gray72"
                            android:textSize="@dimen/_12jdp" />

                        <TextView
                            android:id="@+id/tv_recylrvw_dalalwssls_other"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_5jdp"
                            android:layout_marginStart="@dimen/_5jdp"
                            android:gravity="end"
                            android:textAllCaps="true"
                            android:textColor="@color/gray800"
                            android:textSize="@dimen/_12jdp"
                            android:textStyle="bold"
                            tools:text="0" />
                    </LinearLayout>
                    <!--END Other-->
                </LinearLayout>
                <!--END Madhur & Other-->

                <!--Mobile & Total Bori-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5jdp"
                    android:baselineAligned="false"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <!--Mobile-->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/mobile_coln"
                            android:textColor="@color/gray72"
                            android:textSize="@dimen/_12jdp" />

                        <TextView
                            android:id="@+id/tv_recylrvw_dalalwssls_mobile"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_3jdp"
                            android:layout_marginLeft="@dimen/_5jdp"
                            android:layout_marginRight="@dimen/_3jdp"
                            android:layout_marginStart="@dimen/_5jdp"
                            android:gravity="end"
                            android:textColor="@color/colorPrimaryTeal"
                            android:textSize="@dimen/_12jdp"
                            android:textStyle="bold"
                            tools:text="9662675372" />
                    </LinearLayout>
                    <!--END Mobile-->

                    <!--Total Bori-->
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_2jdp"
                        android:layout_marginStart="@dimen/_2jdp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/totalbori_sml"
                            android:textColor="@color/gray72"
                            android:textSize="@dimen/_12jdp" />

                        <TextView
                            android:id="@+id/tv_recylrvw_dalalwssls_totalbori"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_5jdp"
                            android:layout_marginStart="@dimen/_5jdp"
                            android:gravity="end"
                            android:textColor="@color/colorPrimaryDeepOrange"
                            android:textSize="@dimen/_12jdp"
                            android:textStyle="bold"
                            tools:text="100" />
                    </LinearLayout>
                    <!--END Total Bori-->
                </LinearLayout>
                <!--END Mobile & Total Bori-->
            </LinearLayout>
            <!--END Content-->
    </RelativeLayout>
</android.support.v7.widget.CardView>