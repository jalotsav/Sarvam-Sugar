<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cordntrlyot_godownwise_stock"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".navgtndrawer.FrgmntGodownwiseStock"
    tools:ignore="Missingprefix">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.jalotsav.sarvamsugar.common.RecyclerViewEmptySupport
            android:id="@+id/rcyclrvw_godownwise_stock"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:listitem="@layout/lo_recyclritem_godownwise_stock" />

        <include
            layout="@layout/lo_recyclremptyvw_appearhere"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <ProgressBar
            android:id="@+id/prgrsbr_godownwise_stock_main"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:visibility="gone" />
    </RelativeLayout>

    <!--Filters FAB-->
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab_godownwise_stock_filters"
        style="@style/AppCompatButton.Red"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right|end|bottom"
        android:layout_margin="@dimen/_16jdp"
        android:clickable="true"
        app:useCompatPadding="true" />
</android.support.design.widget.CoordinatorLayout>